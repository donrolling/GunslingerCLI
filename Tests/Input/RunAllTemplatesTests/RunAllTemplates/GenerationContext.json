      "Language": "csharp",

      "DataProviderName": "SQLModelsDataProvider",
      "Imports": [
        "Dapper",
        "Dapper.SQLRepository",
        "Dapper.SQLRepository.Interfaces",
        "Dapper.SQLRepository.Models",
        "sample.data.Entities",
        "Microsoft.Extensions.Logging",
        "System.Threading.Tasks",
        "System.Data"
      ]
    },
    {
      "Type": "Model",
      "Name": "RepositoryInterface",
      "Namespace": "sample.Data.Interfaces",
      "IsStub": false,
      "InputRelativePath": "RepositoryInterface.txt",
      "OutputRelativePath": "sample.data\\Repository\\Interfaces\\I{entityName}Repository.cs",
      "Language": "csharp",

      "DataProviderName": "SQLModelsDataProvider",
      "Imports": [
        "Dapper.SQLRepository.Interfaces",
        "sample.data.Entities"
      ]
    },
    {
      "Type": "Model",
      "Name": "RepositoryTest",
      "Namespace": "sample.Test.DataAccess",
      "IsStub": false,
      "InputRelativePath": "DataAccessTest.txt",
      "OutputRelativePath": "sample.test\\DataAccess\\{entityName}_CRUD_Test.cs",
      "Language": "csharp",

      "DataProviderName": "SQLModelsDataProvider",
      "Imports": [
        "AutoFixture",
        "sample.data.Entities",
        "sample.Data.Interfaces",
        "sample.test.Base",
        "sample.test.Utility",
        "Microsoft.Extensions.DependencyInjection",
        "Microsoft.VisualStudio.TestTools.UnitTesting",
        "System.Collections.Generic",
        "System.Threading.Tasks"
      ]
    },
    {
      "Type": "Model",
      "Name": "InsertProcedure",
      "Namespace": "",

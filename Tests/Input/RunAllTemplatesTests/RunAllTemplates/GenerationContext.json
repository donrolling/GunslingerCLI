      "Language": "csharp",

      "DataProviderName": "SQLModelsDataProvider",
      "Imports": [
        "sample.Data.Repository.Base",
        "sample.Data.Interfaces",
        "Microsoft.Extensions.Logging"
      ]
    },
    {
      "Type": "Model",
      "Name": "BaseRepository",
      "Namespace": "sample.Data.Repository.Base",
      "IsStub": false,
      "InputRelativePath": "BaseRepository.txt",
